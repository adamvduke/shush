{
  "AWSEBDockerrunVersion": 2,
  "authentication": {
    "bucket": "articulate-us-dev",
    "key": "shush/dockercfg"
  },
  "containerDefinitions": [
    {
      "name": "shush",
      "image": "quay.io/articulate/shush:aws-eb",
      "environment": [
        {
          "name": "RACK_ENV",
          "value": "production"
        },
        {
          "name": "SHUSH_HOST",
          "value": "shush.articulate.zone"
        },
        {
          "name": "NOTIFY_FROM",
          "value": "shush@articulate.com"
        },
        {
          "name": "PORT",
          "value": "9393"
        }
      ],
      "essential": true,
      "memory": 786,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 9393
        }
      ]
    }
  ]
}
